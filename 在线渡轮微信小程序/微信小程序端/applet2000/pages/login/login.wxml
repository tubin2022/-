<!-- 头部信息栏 -->
<!-- <view class="top">用户登录</view> -->
<!-- 自定义顶部导航栏 -->
<navigation-bar title="用户登录" typei="3"></navigation-bar>
<!-- body -->
 <van-cell-group>
 <!-- 头部信息栏 -->
<view class="top">用户登录</view>
 <!-- 手机号 -->
  <van-field
    model:value="{{ phone }}"
    placeholder="请输入手机号"
    label="手机"
    required
    clearable
  />
  <!-- 密码 -->
  <van-field
    model:value="{{ password }}"
    placeholder="请输入密码"
    label="密码"
    type="password"
    clearable
    required
  />
</van-cell-group>
   <!-- 登录按钮 -->
   <view class="btn">
      <van-button round type="info" bindtap="goLogin" size="large">登录</van-button>
   </view>
   <!-- 注册 -->
   <view class="xieyi">
     <navigator url="/pages/register/register">用户注册</navigator>
   </view>