\r\n\t\t\t\t// \tvar j = this.current_page;\r\n\t\t\t\t// \t// for(var i=j-1 ;i<j*size;i++ ){\r\n\t\t\t\t// \tthis.current_page_data = this.tableData.slice(j - 1, j * size);\r\n\t\t\t\t// \t// }\r\n\t\t\t\t// },\r\n\t\t\t\t// handleSizeChange(val) {\r\n\t\t\t\t// \tconsole.log(`当前每页${val}条`);\r\n\t\t\t\t// \tthis.onloads();\r\n\t\t\t\t// },\r\n\t\t\t\t// handleCurrentChange(val) {\r\n\t\t\t\t// \tconsole.log(`当前处于${val}页`)\r\n\t\t\t\t// \tconsole.log(this.current_page)\r\n\t\t\r\n\t\t\t\t// \tvar temp = (val - 1) * this.page_size;\r\n\t\t\r\n\t\t\t\t// \tif (temp + this.page_size < this.tableData.length)\r\n\t\t\t\t// \t\tthis.current_page_data = this.tableData.slice(temp, temp + this.page_size)\r\n\t\t\t\t// \telse {\r\n\t\t\t\t// \t\tthis.current_page_data = this.tableData.slice(temp, this.tableData.length)\r\n\t\t\t\t// \t}\r\n\t\t\r\n\t\t\t\t// },\r\n\t\t\r\n\t\t\t\ttoggleSelection(rows) {\r\n\t\t\t\t\tif (rows) {\r\n\t\t\t\t\t\trows.forEach(row => {\r\n\t\t\t\t\t\t\tthis.$refs.multipleTable.toggleRowSelection(row);\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tthis.$refs.multipleTable.clearSelection();\r\n\t\t\t\t\t}\r\n\t\t\t\t},\r\n\t\t\t\thandleSelectionChange(val) {\r\n\t\t\t\t\tthis.multipleSelection = val;\r\n\t\t\r\n\t\t\t\t},\r\n\t\t\t\thandleEdit(index, val) {\r\n\t\t\t\t\tconsole.log(index);\r\n\t\t\t\t\tconsole.log(val);\r\n\t\t\t\t\tval.visible = !val.visible;\r\n\t\t\t\t},\r\n\t\t\t\thandleBatchDelete() {\r\n\t\t\t\t\tif (this.multipleSelection.length) {\r\n\t\t\t\t\t\tfor (let i = 0; i < this.multipleSelection.length; i++) {\r\n\t\t\t\t\t\t\tthis.ids[i] = this.multipleSelection[i].id;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tthis.delete();\r\n\t\t\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tthis.$alert(\"请选择数据\", \"提示\", {\r\n\t\t\t\t\t\t\tconfirmButtonText: '确定',\r\n\t\t\t\t\t\t\ttype: 'info',\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t}\r\n\t\t\t\t},\r\n\t\t\t\tdelete() {\r\n\t\t\t\t\tthis.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {\r\n\t\t\t\t\t\t\tconfirmButtonText: '确定',\r\n\t\t\t\t\t\t\tcancelButtonText: '取消',\r\n\t\t\t\t\t\t\ttype: 'warning'\r\n\t\t\t\t\t\t}).then(() => {\r\n\t\t\t\t\t\t\tconsole.log(this.ids);\r\n\t\t\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\t\t\ttype: 'success',\r\n\t\t\t\t\t\t\t\tmessage: '删除成功!'\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t\tthis.ids = [];\r\n\t\t\t\t\t\t\tthis.multipleSelection = [];\r\n\t\t\t\t\t\t})\r\n\t\t\t\t\t\t.catch(() => {\r\n\t\t\r\n\t\t\t\t\t\t})\r\n\t\t\t\t},\r\n\t\t\t\thandleDelete(index, val) {\r\n\t\t\t\t\tthis.ids[0] = val.id;\r\n\t\t\t\t\tthis.delete();\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\r\n\t\t\tcreated(){\r\n\t\t\r\n\t\t\t\t\t\t\t// this.onloads();\r\n\t\t\t\tthis.init(this.current_page,this.page_size)\r\n\t\t\t}\r\n\t\t}\r\n</script>\r\n\r\n<style>\r\n\t.top {\r\n\t\t/* margin-top: 10px; */\r\n\t\tmargin: auto;\r\n\t\tfont-size: 34px;\r\n\t\tcolor: rgb(154, 217, 217);\r\n\t\tfont-weight: bold;\r\n\t\ttext-align: center;\r\n\t\theight: 50px;\r\n\t\t/* background-color: rgb(253, 255, 254); */\r\n\t}\r\n</style>\n"]}]}